{
  "name": "constructai",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:next": "next dev -p 3002",
    "dev:advanced": "concurrently \"npm run server\" \"npm run dev:next\"",
    "server": "tsx server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "vite build",
    "build:next": "next build",
    "start": "next start -p 3002",
    "preview": "vite preview",
    "deploy": "npm run build && node deploy-ionos.cjs",
    "vercel:dev": "vercel dev",
    "vercel:deploy": "vercel",
    "vercel:prod": "vercel --prod",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "db:backup": "chmod +x scripts/backup-database.sh && ./scripts/backup-database.sh",
    "db:restore": "chmod +x scripts/restore-database.sh && ./scripts/restore-database.sh",
    "db:health": "curl -H \"Authorization: Bearer $(cat .token 2>/dev/null || echo '')\" http://localhost:3001/api/health/database",
    "db:optimize": "tsx server/apply-indexes.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
