{
    "name": "cortexbuildpro.com",
    "private": true,
    "version": "1.1.0",
    "description": "CortexBuild Pro App",
    "main": "server/dist/index.js",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
        "build": "cd server && npm install && npm run build",
        "build:frontend": "vite build",
        "build:frontend:prod": "VITE_API_URL=https://api.cortexbuildpro.com/api VITE_WS_URL=wss://api.cortexbuildpro.com/live vite build",
        "build:backend": "cd server && npm run build",
        "preview": "vite preview",
        "start": "npm start --prefix server",
        "server": "npm run build --prefix server && npm start --prefix server",
        "server:dev": "npm run dev --prefix server",
        "deploy:frontend": "npm run build:frontend && node ./scripts/deploy-frontend-sftp.js",
        "deploy:backend": "npm run build:backend && node ./scripts/deploy-backend-ftp.js && node ./scripts/remote-exec.js",
        "remote:install": "node ./scripts/remote-exec.js",
        "remote:inspect": "node ./scripts/remote-inspect.js",
        "remote:logs": "node ./scripts/remote-logs.js",
        "remote:start": "node ./scripts/remote-start.js",
        "fix:frontend": "node ./scripts/fix-frontend.js",
        "upload:env": "node ./scripts/upload-env.js",
        "seed:adrian": "npm run seed:adrian --prefix server",
        "seed:automations": "npm run seed:automations --prefix server",
        "migrate": "npx tsx server/scripts/migrate.ts",
        "migrate:status": "npx tsx server/scripts/migrate.ts status",
        "lint": "eslint \"**/*.{ts,tsx}\"",
        "typecheck": "tsc --noEmit",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage"
    },
    "dependencies": {
        "@google/genai": "^1.34.0",
        "@google/generative-ai": "^0.24.1",
        "@sendgrid/mail": "^8.1.6",
        "@supabase/supabase-js": "^2.39.0",
        "@tanstack/react-query": "^5.90.12",
        "@tanstack/react-query-devtools": "^5.91.1",
        "apollo-server-express": "^3.13.0",
        "bcryptjs": "^3.0.3",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^4.18.2",
        "express-rate-limit": "^8.2.1",
        "graphql": "^16.12.0",
        "helmet": "^8.1.0",
        "hostinger-api-sdk": "^0.0.17",
        "html2canvas": "^1.4.1",
        "idb": "^8.0.3",
        "jsonwebtoken": "^9.0.3",
        "jspdf": "^4.0.0",
        "leaflet": "1.9.4",
        "lottie-react": "2.4.1",
        "lucide-react": "^0.554.0",
        "multer": "^2.0.2",
        "mysql2": "^3.16.0",
        "onnxruntime-web": "^1.19.0",
        "pg": "^8.16.3",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-router-dom": "^7.11.0",
        "recharts": "^3.6.0",
        "ssh2-sftp-client": "^12.0.1",
        "strip-comments": "^2.0.1",
        "tsx": "^4.21.0",
        "uuid": "^11.0.0",
        "web-push": "^3.6.7",
        "winston": "^3.19.0",
        "ws": "^8.18.3",
        "zod": "^4.2.1"
    },
    "devDependencies": {
        "@apollo/client": "^4.0.11",
        "@graphql-tools/schema": "^10.0.30",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/bcryptjs": "^2.4.6",
        "@types/express": "^5.0.6",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/multer": "^2.0.0",
        "@types/node": "^22.14.0",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.50.0",
        "@typescript-eslint/parser": "^8.50.0",
        "@vitejs/plugin-react": "^5.0.0",
        "@vitest/ui": "^4.0.16",
        "autoprefixer": "^10.4.23",
        "basic-ftp": "^5.1.0",
        "concurrently": "^9.2.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.26",
        "fs-extra": "^11.3.3",
        "husky": "^9.1.7",
        "jsdom": "^27.3.0",
        "jsonfile": "^6.2.0",
        "lint-staged": "^16.2.7",
        "postcss": "^8.5.6",
        "prettier": "^3.7.4",
        "puppeteer": "^24.34.0",
        "sqlite": "^5.1.1",
        "sqlite3": "^5.1.7",
        "ssh2": "^1.17.0",
        "supertest": "^7.1.4",
        "tailwindcss": "^3.4.1",
        "typescript": "~5.8.2",
        "vite": "^6.0.0",
        "vite-plugin-pwa": "^1.2.0",
        "vitest": "^4.0.16",
        "workbox-core": "^7.4.0",
        "workbox-precaching": "^7.4.0",
        "workbox-routing": "^7.4.0",
        "workbox-strategies": "^7.4.0"
    },
    "overrides": {
        "path-to-regexp": "^8.3.0",
        "undici": "^7.16.0"
    }
}